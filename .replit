modules = ["python-3.11", "ruby-3.2", "web"]

[nix]
channel = "stable-25_05"
packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "imagemagickBig", "lcms2", "libGL", "libGLU", "libaom", "libde265", "libheif", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "nasm", "openjpeg", "pkg-config", "qhull", "tcl", "tesseract", "tk", "x265", "xsimd", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["bash", "-c", "PORT=5000 python api_server.py"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DEEP ANAL App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "OpenAI API Server"

[[workflows.workflow]]
name = "DEEP ANAL App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run main.py --server.address=0.0.0.0 --server.port=5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "OpenAI API Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python api_server.py"

[workflows.workflow.metadata]
outputType = "console"

